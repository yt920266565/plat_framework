<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yanmade.plat.report.dao.ImportFileLogMapper">

    <insert id="insertLog" parameterType="com.yanmade.plat.report.entity.ImportFileLog">
        insert into import_file_log values(#{count}, #{auto}, #{username}, #{createtime})
    </insert>

    <select id="selectLog" parameterType="int" resultType="com.yanmade.plat.report.entity.ImportFileLog">
        select * from import_file_log order by createtime desc limit 0, #{count}
    </select>

</mapper>